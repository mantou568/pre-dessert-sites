  "requests": {
    "search": {
      "fields": {
        "hr_days": {
          "selector": "img.hitandrun",
          "attribute": "title",
          "filters": [
            {
              "name": "not_blank",
              "args": [
                "3",
                "0"
              ]
            }
          ]
        },
        "labels": {
          "selector": "td.embedded > span[title]",
          "array": true
        },
        "description": {
          "selector": "td.embedded:nth-child(1)",
          "remove": "span,a,img,font,b,br",
          "contents": -1
        }
      }
    }
  },
  "my_hr": {
    "path": "/myhr.php?status=1"
  }
