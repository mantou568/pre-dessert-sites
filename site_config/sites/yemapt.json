{
  "id": "yemapt",
  "name": "野马",
  "domain": "https://www.yemapt.org",
  "encoding": "UTF-8",
  "schema": "NexusPHP",
  "reuse_schema": "NexusPHP",
  "required": {
    "cookie": true,
    "sign_in": false
  },
  "meta": {
    "has_free": true
  },
  "public": false,
  "category": {
    "movie": [
      {
        "id": 4,
        "cat": "Movies",
        "desc": "电影"
      }
    ],
    "tv": [
      {
        "id": 5,
        "cat": "TV",
        "desc": "剧集"
      },
      {
        "id": 14,
        "cat": "TV/Anime",
        "desc": "动漫"
      },
      {
        "id": 21,
        "cat": "TV/Other",
        "desc": "教育视频"
      }
    ]
  },
  "requests": {
    "favicon": {
      "parser": "None",
      "method": "GET",
      "path": "/favicon.ico"
    },
    "search": {
      "parser": "JsonPath",
      "method": "POST",
      "path": "/api/torrent/fetchOpenTorrentList",
      "headers": {
        "Content-Type": "application/json"
      },
      "body": {
        "keyword": "{keyword}",
        "pageParam": {
          "current": "{page}",
          "pageSize": 40,
          "pageSizeOptions": [
            "10",
            "20",
            "40"
          ],
          "size": "small"
        },
        "sorter": {}
      },
      "list": {
        "selector": "data"
      },
      "disabled_extends": {
        "params": true
      },
      "fields": {
        "id": {
          "selector": "id"
        },
        "title": {
          "selector": "showName"
        },
        "details": {
          "selector": "id",
          "filters": [
            {
              "name": "append_left",
              "args": "/#/torrent/detail/"
            }
          ]
        },
        "download": {
          "selector": "id",
          "filters": [
            {
              "name": "append_left",
              "args": "/api/torrent/download/"
            }
          ]
        },
        "size": {
          "selector": "fileSize"
        },
        "seeders": {
          "selector": "seedNum"
        },
        "leechers": {
          "selector": "leechNum"
        },
        "date_added": {
          "selector": "listingTime",
          "filters": [
            {
              "name": "timestamp"
            }
          ]
        },
        "downloadvolumefactor": {
          "selector": "downloadPromotion",
          "filters": [
            {
              "name": "case",
              "args": {
                "free": 0,
                "half": 0.5,
                "percent_70": 0.3,
                "normal": 1,
                "none": 1
              }
            }
          ]
        },
        "uploadvolumefactor": {
          "selector": "uploadPromotion",
          "filters": [
            {
              "name": "case",
              "args": {
                "2x": 2,
                "normal": 1,
                "none": 1
              }
            }
          ]
        },
        "description": {
          "selector": "shortDesc"
        },
        "labels": {
          "selector": "tagList",
          "array": true,
          "filters": [
            {
              "name": "case",
              "args": {
                "0": "",
                "1": "",
                "2": "",
                "4": "",
                "5": "国语",
                "6": "中字",
                "7": "粤语",
                "8": "英字",
                "9": "DIY",
                "10": "杜比视界",
                "11": "分集",
                "12": "完结"
              }
            }
          ]
        }
      }
    },
    "user_basic_info": {
      "parser": "JsonPath",
      "method": "GET",
      "path": "/api/consumer/fetchSelfDetail",
      "headers": {
        "Content-Type": "application/json",
        "Referer": "https://www.yemapt.org/"
      },
      "fields": {
        "id": {
          "selector": "data.id"
        },
        "name": {
          "selector": "data.name"
        },
        "uploaded": {
          "selector": "data.promotionUploadSize"
        },
        "downloaded": {
          "selector": "data.promotionDownloadSize"
        },
        "bonus": {
          "selector": "data.bonus"
        }
      }
    },
    "user_details": {
      "parser": "JsonPath",
      "method": "GET",
      "path": "/api/consumer/fetchSelfDetail",
      "headers": {
        "Content-Type": "application/json",
        "Referer": "https://www.yemapt.org/"
      },
      "fields": {
        "level": {
          "selector": "data.level"
        },
        "level_id": {
          "selector": "data.level"
        },
        "join_at": {
          "selector": "data.registerTime",
          "filters": [
            {
              "name": "timestamp"
            }
          ]
        },
        "last_accessed": {
          "selector": "data.lastLoginInfo.date",
          "filters": [
            {
              "name": "timestamp"
            }
          ]
        }
      }
    },
    "seeding_size": {
      "parser": "JsonPath",
      "method": "POST",
      "path": "/api/userTorrent/fetchSeedTorrentInfo",
      "fields": {
        "size": {
          "selector": "data.fileSize",
          "filters": [
            {
              "name": "byte_size"
            }
          ]
        }
      }
    },
    "seeding_count": {
      "parser": "JsonPath",
      "method": "POST",
      "path": "/api/userTorrent/fetchSeedTorrentInfo",
      "fields": {
        "count": {
          "selector": "data.num"
        }
      }
    }
  }
}