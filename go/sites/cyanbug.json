{
  "id": "cyanbug",
  "name": "大青虫",
  "domain": "https://cyanbug.net",
  "public": false,
  "schema": "NexusPHP",
  "required": {
    "cookie": true,
    "sign_in": true
  },
  "meta": {
    "has_free": true,
    "has_2x_free": true,
    "has_cover": true,
    "shoutbox": true
  },
  "category": {
    "movie": [
      {
        "id": 401,
        "cat": "Movies",
        "desc": "Movies(电影)",
        "default": true
      },
      {
        "id": 406,
        "cat": "Movies",
        "desc": "Music Videos/音乐视频"
      },
      {
        "id": 407,
        "cat": "Movies/Sports",
        "desc": "Sports/体育"
      }
    ],
    "tv": [
      {
        "id": 402,
        "cat": "TV",
        "desc": "TV Series(电视剧)",
        "default": true
      },
      {
        "id": 403,
        "cat": "TV",
        "desc": "TV Shows(综艺)",
        "default": true
      },
      {
        "id": 404,
        "cat": "TV/Documentary",
        "desc": "Documentaries(纪录片)",
        "default": true
      },
      {
        "id": 405,
        "cat": "TV/Anime",
        "desc": "Animations(动画)",
        "default": true
      }
    ]
  },
  "search_box": {
    "fields": [
      {
        "title": "媒介",
        "name": "medium",
        "type": "checkbox",
        "options": [
          {
            "text": "Other",
            "value": "medium12"
          },
          {
            "text": "Track",
            "value": "medium9"
          },
          {
            "text": "CD",
            "value": "medium8"
          },
          {
            "text": "DVDR",
            "value": "medium6"
          },
          {
            "text": "MiniBD",
            "value": "medium4"
          },
          {
            "text": "HDTV",
            "value": "medium5"
          },
          {
            "text": "HD DVD",
            "value": "medium2"
          },
          {
            "text": "WEB-DL",
            "value": "medium10"
          },
          {
            "text": "Encode",
            "value": "medium7"
          },
          {
            "text": "Remux",
            "value": "medium3"
          },
          {
            "text": "UHD Blu-ray",
            "value": "medium11"
          },
          {
            "text": "Blu-ray",
            "value": "medium1"
          }
        ]
      },
      {
        "title": "分辨率",
        "name": "standard",
        "type": "checkbox",
        "options": [
          {
            "text": "SD",
            "value": "standard4"
          },
          {
            "text": "720p",
            "value": "standard3"
          },
          {
            "text": "1080i",
            "value": "standard2"
          },
          {
            "text": "1080p",
            "value": "standard1"
          },
          {
            "text": "2160p",
            "value": "standard5"
          }
        ]
      }
    ]
  },
  "requests": {
    "seeding_statistics": {
      "parser": "CssSelector",
      "disabled_extends": {
        "headers": true,
        "list": true
      },
      "fields": {
        "size": {
          "selector": "div > div:nth-child(1)",
          "remove": "b",
          "filters": [
            {
              "name": "re_search",
              "args": [
                "总大小：(.*)",
                1
              ]
            },
            {
              "name": "byte_size"
            }
          ]
        },
        "count": {
          "selector": "div > div:nth-child(1) > b"
        }
      }
    }
  }
}
