{
  "id": "52pt",
  "name": "52pt",
  "domain": "https://52pt.site",
  "public": false,
  "schema": "pt52pt",
  "reuse_schema": "NexusPHP",
  "required": {
    "cookie": true,
    "sign_in": true
  },
  "meta": {
    "has_free": true,
    "has_2x_free": true,
    "has_hr": true
  },
  "category": {
    "movie": [
      {
        "id": 401,
        "cat": "Movies",
        "desc": "Movies/电影"
      },
      {
        "id": 406,
        "cat": "Movies",
        "desc": "Music Videos/音乐视频"
      },
      {
        "id": 407,
        "cat": "Movies/Sports",
        "desc": "Sports/体育"
      },
      {
        "id": 410,
        "cat": "Movies/Opera",
        "desc": "Opera/戏曲 曲艺"
      },
      {
        "id": 411,
        "cat": "Movies/Erotic",
        "desc": "Erotic/18+"
      }
    ],
    "tv": [
      {
        "id": 402,
        "cat": "TV",
        "desc": "TV Series/连续剧"
      },
      {
        "id": 403,
        "cat": "TV",
        "desc": "TV Shows/综艺"
      },
      {
        "id": 404,
        "cat": "TV/Documentary",
        "desc": "Documentaries/纪录片"
      },
      {
        "id": 405,
        "cat": "TV/Anime",
        "desc": "Animations/动漫"
      }
    ]
  },
  "requests": {
    "search": {
      "fields": {
        "hr_days":{
          "filters": [
            {
              "name": "constant",
              "args": "1"
            }
          ]
        },
        "description": {
          "selector": "td.embedded:nth-child(1)",
          "remove": "span,a,img,font,b,br",
          "contents": -1
        }
      }
    },
    "sign_in_page": {
      "parser": "None",
      "method": "GET",
      "path": "/bakatest.php",
      "disabled_extends": {
        "params": true
      },

      "headers": {
        "Referer": "https://52pt.site/torrents.php"
      }
    },
   "sign_in_submit": {
      "parser": "JsonPath",
      "method": "POST",
      "path": "/bakatest.php",
      "disabled_extends": {
        "params": true
      },
      "form_data": {
        "questionid": [
          "{questionid}"
        ],
        "choice[]": [
          "{choice[]}"
        ],
        "usercomment": [
          "{usercomment}"
        ]
      },
      "headers": {
        "Referer": "https://52pt.site/bakatest.php",
        "content-type": "application/x-www-form-urlencoded"
      }
    },
    "seeding_statistics": {
      "fields": {
        "size": {
          "selector": "/td[3]",
          "filters": [
            {
              "name": "byte_size"
            }
          ]
        }
      }
    },
    "my_hr": {
      "path": "/myhr.php?hrtype=C"
    }
  }
}
