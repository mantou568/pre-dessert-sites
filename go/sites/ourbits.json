{
  "id": "ourbits",
  "name": "我堡",
  "domain": "https://ourbits.club",
  "additional_domain": ["https://pt.ourhelp.club"],
  "public": false,
  "schema": "NexusPHP",
  "required": {
    "cookie": true,
    "sign_in": true
  },
  "meta": {
    "has_free": true,
    "has_2x_free": true,
    "has_hr": true
  },
  "category": {
    "query": "cat[]",
    "movie": [
      {
        "id": 401,
        "cat": "Movies",
        "desc": "Movies"
      },
      {
        "id": 402,
        "cat": "Movies/3D",
        "desc": "Movies 3D"
      }
    ],
    "tv": [
      {
        "id": 405,
        "cat": "TV",
        "desc": "TV Packs"
      },
      {
        "id": 410,
        "cat": "TV/Documentary",
        "desc": "Documentaries"
      },
      {
        "id": 411,
        "cat": "TV/Anime",
        "desc": "Animations"
      },
      {
        "id": 412,
        "cat": "TV",
        "desc": "TV Episodes"
      },
      {
        "id": 413,
        "cat": "TV",
        "desc": "TV Shows"
      },
      {
        "id": 419,
        "cat": "TV",
        "desc": "Concert"
      }
    ]
  },
  "requests": {
    "user_details": {
      "fields": {
        "last_accessed": {
          "selector": "//*[@id='outer']/table/tbody/tr/td/table/tbody/tr[4]/td[2]/text()[1]",
          "filters": [
            {
              "name": "re_search",
              "args": [
                "^网站访问: (\\d{4}-\\d{2}-\\d{2} \\d{2}:\\d{2}:\\d{2}) \\($",
                1
              ]
            },
            {
              "name": "timestamp"
            }
          ]
        }
      }
    },
    "search": {
      "fields": {
        "description": {
          "selector": "table.torrentname tr > td.embedded",
          "remove": "div,a,img,b",
          "filters": [
            {
              "name": "replace",
              "args": [
                "剩余时间：",
                ""
              ]
            }
          ]
        },
        "labels": {
          "array": true,
          "selector": "table.torrentname tr > td.embedded > div > a[href*='?tags[]='] > span"
        },
        "size": {
          "selector": "td.rowfollow:nth-child(5)",
          "filters": [
            {
              "name": "byte_size"
            }
          ]
        },
        "hr_days": {
          "selector": "img.hitandrun",
          "attribute": "title",
          "filters": [
            {
              "name": "not_blank",
              "args": [
                "2",
                "0"
              ]
            }
          ]
        }
      }
    },
    "seeding_statistics": {
      "fields": {
        "size": {
          "selector": "/td[3]",
          "filters": [
            {
              "name": "byte_size"
            }
          ]
        }
      }
    }
  }
}
