{
  "id": "ttg",
  "name": "听听歌",
  "domain": "https://totheglory.im",
  "public": false,
  "schema": "ttg",
  "reuse_schema": "NexusPHP",
  "required": {
    "cookie": true,
    "sign_in": true
  },
  "meta": {
    "has_free": true,
    "has_hr": true
  },
  "category": {
    "movie": [
      {
        "id": "电影DVDRip",
        "cat": "Movies/SD"
      },
      {
        "id": "电影720p",
        "cat": "Movies/HD"
      },
      {
        "id": "电影1080i/p",
        "cat": "Movies/HD"
      },
      {
        "id": "BluRay原盘",
        "cat": "Movies/BluRay"
      },
      {
        "id": "影视2160p",
        "cat": "Movies/UHD"
      },
      {
        "id": "UHD原盘",
        "cat": "Movies/UHD"
      }
    ],
    "tv": [
      {
        "id": "纪录片720p",
        "cat": "Movies/HD"
      },
      {
        "id": "纪录片1080i/p",
        "cat": "Movies/HD"
      },
      {
        "id": "纪录片BluRay原盘",
        "cat": "Movies/BluRay"
      },
      {
        "id": "欧美剧720p",
        "cat": "TV/HD"
      },
      {
        "id": "欧美剧1080i/p",
        "cat": "TV/HD"
      },
      {
        "id": "高清日剧",
        "cat": "TV/HD"
      },
      {
        "id": "大陆港台剧1080i/p",
        "cat": "TV/HD"
      },
      {
        "id": "大陆港台剧720p",
        "cat": "TV/HD"
      },
      {
        "id": "高清韩剧",
        "cat": "TV/HD"
      },
      {
        "id": "欧美剧包",
        "cat": "TV/HD"
      },
      {
        "id": "日剧包",
        "cat": "TV/HD"
      },
      {
        "id": "韩剧包",
        "cat": "TV/HD"
      },
      {
        "id": "华语剧包",
        "cat": "TV/HD"
      },
      {
        "id": "高清动漫",
        "cat": "TV/HD"
      },
      {
        "id": "韩国综艺",
        "cat": "TV/HD"
      },
      {
        "id": "日本综艺",
        "cat": "TV/HD"
      },
      {
        "id": "高清综艺",
        "cat": "TV/HD"
      },
      {
        "id": "动漫原盘",
        "cat": "TV/BluRay"
      }
    ]
  },
  "requests": {
    "search": {
      "parser": "CssSelector",
      "method": "GET",
      "path": "/browse.php",
      "params": {
        "c": [
          "M"
        ]
      },
      "list": {
        "selector": "table#torrent_table tr[id]"
      },
      "fields": {
        "id": {
          "selector": "div.name_left > a",
          "attribute": "href",
          "filters": [
            {
              "name": "re_search",
              "args": [
                "\\d+",
                0
              ]
            }
          ]
        },
        "description": {
          "any": [
            {
              "selector": "div.name_left > a > b > font > span"
            },
            {
              "selector": "div.name_left > a > b > span"
            }
          ]
        },
        "labels": {
          "array": true,
          "selector": "div.name_left > span > b"
        },
        "title": {
          "selector": "div.name_left > a > b",
          "remove": "span",
          "filters": [
            {
              "name": "split",
              "args": [
                "\n",
                0
              ]
            }
          ]
        },
        "category": {
          "selector": "tr[id] td:nth-child(1) > a > img",
          "attribute": "alt"
        },
        "details": {
          "selector": "div.name_left > a",
          "attribute": "href"
        },
        "download": {
          "selector": "a.dl_a",
          "attribute": "href"
        },
        "size": {
          "selector": "td:nth-child(7)",
          "filters": [
            {
              "name": "byte_size"
            }
          ]
        },
        "seeders": {
          "selector": "td:nth-child(9) > b > a >font"
        },
        "leechers": {
          "selector": "td:nth-child(9) > b:nth-child(2) > a"
        },
        "grabs": {
          "selector": "td:nth-child(8)",
          "remove": "br",
          "filters": [
            {
              "name": "replace",
              "args": [
                "次",
                ""
              ]
            }
          ]
        },
        "date_added": {
          "selector": "td:nth-child(5) > nobr",
          "filters": [
            {
              "name": "timestamp",
              "args": [
                "2006-01-02 15:04:05",
                "2006-01-0215:04:05"
              ]
            }
          ]
        },
        "downloadvolumefactor": {
          "any": [
            {
              "selector": "img[alt='free']",
              "attribute": "alt",
              "filters": [
                {
                  "name": "not_blank",
                  "args": [
                    0,
                    ""
                  ]
                }
              ]
            },
            {
              "selector": "img[alt='50%']",
              "attribute": "alt",
              "filters": [
                {
                  "name": "not_blank",
                  "args": [
                    0.5,
                    ""
                  ]
                }
              ]
            },
            {
              "selector": "img[alt='30%']",
              "attribute": "alt",
              "filters": [
                {
                  "name": "not_blank",
                  "args": [
                    0.3,
                    1
                  ]
                }
              ]
            }
          ]
        },
        "uploadvolumefactor": {
          "any": [
            {
              "selector": "img[alt='200%']",
              "attribute": "alt",
              "filters": [
                {
                  "name": "not_blank",
                  "args": [
                    2,
                    1
                  ]
                }
              ]
            }
          ]
        },
        "hr_days": {
          "optional": true,
          "selector": "td:nth-child(2) > div.name_left > img:nth-child(6)",
          "attribute": "title",
          "filters": [
            {
              "name": "re_search",
              "args": [
                "\\d*",
                0
              ]
            }
          ]
        }
      }
    },
    "details": {
      "parser": "XPath",
      "method": "GET",
      "path": "/t/{id}/",
      "fields": {
        "absent": {
          "selector": "*",
          "selection": "html",
          "filters": [
            {
              "name": "regex",
              "args": "没有此 ID 的种子。"
            }
          ]
        },
        "free": {
          "selector": "//img[@class='topic'][contains(@src,'ico_free.gif')]",
          "filters": [
            {
              "name": "not_blank"
            }
          ]
        },
        "hr": {
          "selector": "//img[@src='/pic/hit_run.gif']",
          "filters": [
            {
              "name": "not_blank"
            }
          ]
        },
        "peer_count": {
          "selector": "//span[@id='dlstatus']",
          "filters": [
            {
              "name": "re_search",
              "args": [
                "\\d+",
                0
              ]
            }
          ]
        }
      }
    },
    "user_basic_info": {
      "fields": {
        "uploaded": {
          "selector": "/html/body/table[2]/tbody/tr/td/table/tbody/tr/td[1]/span/font[4]/a/text()",
          "filters": [
            {
              "name": "byte_size"
            }
          ]
        },
        "downloaded": {
          "selector": "/html/body/table[2]/tbody/tr/td/table/tbody/tr/td[1]/span/font[6]/a/text()",
          "filters": [
            {
              "name": "byte_size"
            }
          ]
        }
      }
    },
    "ttg_sign_in_start_page": {
      "parser": "None",
      "method": "GET",
      "path": "/index.php",
      "disabled_extends": {
        "params": true
      },

      "headers": {
        "Referer": "https://totheglory.im/index.php"
      }
    },
   "ttg_sign_in_submit": {
      "parser": "JsonPath",
      "method": "POST",
      "path": "/signed.php",
      "disabled_extends": {
        "params": true
      },
      "form_data": {
        "signed_timestamp": [
          "{signed_timestamp}"
        ],
        "signed_token": [
          "{signed_token}"
        ]
      },
      "headers": {
        "Referer": "https://totheglory.im/index.php",
        "content-type": "application/x-www-form-urlencoded"
      }
    },
    "seeding_statistics": {
      "parser": "XPath",
      "method": "GET",
      "path": "/userdetails.php?id={userId}",
      "list": {
        "selector": "//td[contains(text(), ‘当前上传’)]/following-sibling::td/table//tr[position()>1]",
        "next_page": {
          "selector": "//a[contains(text(), '下一页') or contains(text(), '下一頁')]/@href"
        }
      },
      "fields": {
        "size": {
          "selector": "/td[4]",
          "filters": [
            {
              "name": "byte_size"
            }
          ]
        }
      }
    }
  }
}
