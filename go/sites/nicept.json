  "requests": {
    "search": {
      "fields": {
        "hr_days": {
          "filters": [
            {
              "name": "constant",
              "args": "6"
            }
          ]
        },
        "description": {
          "selector": "td:nth-child(2) > table > tbody > tr > td:nth-child(2)",
          "remove": "span,a,img,font,b",
          "filters": [
            {
              "name": "split",
              "args": [
                "\n",
                -1
              ]
            },
            {
              "name": "replace",
              "args": ["剩餘時間：", ""]
            }
          ]
        }
      }
    }
  },
  "my_hr": {
    "path": "/myhr.php?status=1"
  }
