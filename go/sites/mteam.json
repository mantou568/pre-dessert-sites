  "requests": {
    "get_subtitle_url": {
      "disabled": true
    },
    "search": {
      "parser": "JsonPath",
      "method": "POST",
      "path": "/api/torrent/search",
      "use_api": true,
      "required_headers": [
        "x-api-key"
      ],
      "list": {
        "selector": "data.data"
      },
      "fields": {
        "id": {},
        "category": {},
        "title": {
          "selector": "name"
        },
        "cover": {
          "selector": "imageList.[0]"
        },
        "details": {
          "selector": "id",
          "filters": [
            {
              "name": "append_left",
              "args": "/detail/"
            }
          ]
        },
        "size": {},
        "grabs": {
          "selector": "status.timesCompleted"
        },
        "seeders": {
          "selector": "status.seeders"
        },
        "leechers": {
          "selector": "status.leechers"
        },
        "date_added": {
          "selector": "createdDate",
          "filters": [
            {
              "name": "timestamp"
            }
          ]
        },
        "downloadvolumefactor": {
          "selector": "status.discount",
          "filters": [
            {
              "name": "case",
              "args": {
                "FREE": "0",
                "PERCENT_50": "0.5",
                "PERCENT_70": "0.3",
                "NORMAL": "1"
              }
            }
          ]
        },
        "uploadvolumefactor": {
          "filters": [
            {
              "name": "constant",
              "args": "1"
            }
          ]
        },
        "description": {
          "selector": "smallDescr"
        },
        "labels": {
          "array": true,
          "selector": "labelsNew"
        },
        "imdb_id": {
          "selector": "imdb",
          "filters": [
            {
              "name": "re_search",
              "args": ["tt(\\d+)", 1]
            }
          ]
        }
      }
    },
    "profile": {
      "parser": "JsonPath",
      "method": "POST",
      "path": "/api/member/profile",
      "use_api": true,
      "required_headers": [
        "x-api-key"
      ],
      "headers": {
        "Content-Type": "application/x-www-form-urlencoded"
      },
      "form_data": {
        "uid": [
          "{userId}"
        ]
      },
      "fields": {
        "created_date": {
          "selector": "data.createdDate",
          "filters": [
            {
              "name": "timestamp"
            }
          ]
        },
        "last_modified_date": {
          "selector": "data.memberStatus.lastModifiedDate",
          "filters": [
            {
              "name": "timestamp"
            }
          ]
        },
        "username": {
          "selector": "data.username"
        },
        "uploaded": {
          "selector": "data.memberCount.uploaded"
        },
        "downloaded": {
          "selector": "data.memberCount.downloaded"
        },
        "share_rate": {
          "selector": "data.memberCount.shareRate"
        },
        "bonus": {
          "selector": "data.memberCount.bonus"
        },
        "role": {
          "selector": "data.role"
        }
      }
    },
    "my_peer_statistics": {
      "parser": "JsonPath",
      "method": "POST",
      "path": "/api/tracker/myPeerStatistics",
      "use_api": true,
      "required_headers": [
        "x-api-key"
      ],
      "headers": {
        "Content-Type": "application/x-www-form-urlencoded"
      },
      "params": {
        "uid": [
          "{userId}"
        ]
      },
      "fields": {
        "uid": {
          "selector": "data.uid"
        },
        "leecherCount": {
          "selector": "data.leecherCount"
        },
        "leecherSize": {
          "selector": "data.leecherSize"
        },
        "seederCount": {
          "selector": "data.seederCount"
        },
        "seederSize": {
          "selector": "data.seederSize"
        },
        "uploadCount": {
          "selector": "data.uploadCount"
        }
      }
    },
    "sys_role_list": {
      "parser": "JsonPath",
      "method": "POST",
      "path": "/api/member/sysRoleList",
      "use_api": true,
      "required_headers": [
        "x-api-key"
      ],
      "list": {
        "selector": "data"
      },
      "fields": {
        "id": {},
        "name_chs": {
          "selector": "nameChs"
        },
        "name_eng": {
          "selector": "nameEng"
        },
        "image": {
          "filters": [
            {
              "name": "append_left",
              "args": "/static/roles/"
            }
          ]
        }
      }
    },
    "notice": {
      "parser": "JsonPath",
      "method": "POST",
      "path": "/api/news/list",
      "use_api": true,
      "required_headers": [
        "x-api-key"
      ],
      "list": {
        "selector": "data"
      },
      "fields": {
        "title": {
          "selector": "subject"
        },
        "date": {
          "selector": "createdDate"
        },
        "content": {}
      }
    },
    "gen_dl_token": {
      "parser": "JsonPath",
      "method": "POST",
      "path": "/api/torrent/genDlToken",
      "use_api": true,
      "required_headers": [
        "x-api-key"
      ],
      "headers": {
        "Content-Type": "application/x-www-form-urlencoded"
      },
      "form_data": {
        "id": [
          "{id}"
        ]
      },
      "fields": {
        "url": {
          "selector": "data"
        }
      }
    },
    "details": {
      "parser": "JsonPath",
      "method": "POST",
      "path": "/api/torrent/detail",
      "use_api": true,
      "required_headers": [
        "x-api-key"
      ],
      "headers": {
        "Content-Type": "application/x-www-form-urlencoded"
      },
      "form_data": {
        "id": [
          "{id}"
        ]
      },
      "fields": {
        "absent": {
          "selector": "message",
          "filters": [
            {
              "name": "eq",
              "args": "種子未找到"
            }
          ]
        },
        "free": {
          "selector": "status.discount",
          "filters": [
            {
              "name": "eq",
              "args": "FREE"
            }
          ]
        },
        "peer_count": {
          "selector": "status.seeders"
        }
      }
    }
  }
}